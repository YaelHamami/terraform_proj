{
  "security_rules": [
    {
      "name": "allow_TCP_in",
      "priority": 110,
      "direction": "Inbound",
      "access": "Allow",
      "protocol": "Tcp",
      "source_port_range": "*",
      "destination_port_range": "22",
      "source_address_prefix": "10.0.0.0/16",
      "destination_address_prefix": "20.0.3.0/24"
    },
    {
      "name": "forward_all_to_firewall",
      "priority": 115,
      "direction": "Inbound",
      "access": "Allow",
      "protocol": "*",
      "source_port_range": "*",
      "destination_port_range": "*",
      "source_address_prefix": "0.0.0.0/0",
      "destination_address_prefix": "${fw_public_ip}"
    },
    {
      "name": "deny_all_in",
      "priority": 120,
      "direction": "Inbound",
      "access": "Deny",
      "protocol": "*",
      "source_port_range": "*",
      "destination_port_range": "*",
      "source_address_prefix": "*",
      "destination_address_prefix": "*"
    }
  ]
}